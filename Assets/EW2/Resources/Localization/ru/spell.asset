    values:
    - "\u041C\u0435\u0442\u0435\u043B\u044C"
    - "\u041F\u044B\u043B\u0430\u044E\u0449\u0438\u0439 \u0433\u0440\u043E\u043C"
    - "\u0414\u0443\u0445 \u0412\u0437\u0440\u044B\u0432\u0430"
    - "\u0417\u0432\u0435\u0437\u0434\u043D\u044B\u0439 \u0421\u0442\u0440\u0430\u0436"
    - "\u0411\u043B\u0430\u0433\u043E\u0441\u043B\u043E\u0432\u0435\u043D\u0438\u0435
      \u043F\u0440\u0438\u0440\u043E\u0434\u044B"
    - "\u041C\u0430\u0433\u0438\u0447\u0435\u0441\u043A\u0430\u044F \u0417\u0435\u043C\u043D\u0430\u044F
      \u043C\u0438\u043D\u0430"
    - "\u0414\u0440\u0435\u0432\u043D\u0438\u0439 \u0442\u043E\u0442\u0435\u043C"
    - "\u0417\u0430\u043C\u043E\u0440\u0430\u0436\u0438\u0432\u0430\u0435\u0442 \u0432\u0440\u0430\u0433\u043E\u0432
      \u0432 \u0432\u044B\u0431\u0440\u0430\u043D\u043D\u043E\u0439 \u043E\u0431\u043B\u0430\u0441\u0442\u0438
      \u043D\u0430 \u043D\u0435\u0441\u043A\u043E\u043B\u044C\u043A\u043E<color=#bb5c14>{0}</color>
      \u0441\u0435\u043A\u0443\u043D\u0434. \u041F\u043E\u0441\u043B\u0435 \u0442\u0430\u044F\u043D\u0438\u044F
      \u043B\u044C\u0434\u0430 \u0432\u0440\u0430\u0433\u0438 \u0431\u0443\u0434\u0443\u0442
      \u0445\u043E\u043B\u043E\u0434\u043D\u044B\u043C\u0438 \u0432 \u0442\u0435\u0447\u0435\u043D\u0438\u0435
      <color=#bb5c14>{1}</color> \u0441\u0435\u043A\u0443\u043D\u0434.\n\n* \u0418\u043C\u043C\u0443\u043D\u0438\u0442\u0435\u0442
      \u043A \u043B\u0435\u0442\u0430\u044E\u0449\u0438\u043C \u0432\u0440\u0430\u0433\u0430\u043C"
    - "\u0421\u043E\u0437\u0434\u0430\u0432\u0430\u0439\u0442\u0435 \u0433\u0440\u043E\u043C
      \u0438 \u043E\u0433\u043E\u043D\u044C, \u043A\u043E\u0442\u043E\u0440\u044B\u0435
      \u043F\u0430\u0434\u0430\u044E\u0442 \u043D\u0430 \u0432\u044B\u0431\u0440\u0430\u043D\u043D\u0443\u044E
      \u043E\u0431\u043B\u0430\u0441\u0442\u044C \u0438 \u043D\u0430\u043D\u043E\u0441\u044F\u0442<color=#bb5c14>{0}</color>
      \u0443\u0440\u043E\u043D"
    - "\u0421\u043E\u0437\u0434\u0430\u0435\u0442 \u0432\u0437\u0440\u044B\u0432,
      \u043A\u043E\u0442\u043E\u0440\u044B\u0439 \u043D\u0430\u043D\u043E\u0441\u0438\u0442
      <color=#bb5c14>{0}</color> \u0443\u0440\u043E\u043D \u0432\u0440\u0430\u0433\u0430\u043C
      \u0432 \u0432\u044B\u0431\u0440\u0430\u043D\u043D\u043E\u0439 \u043E\u0431\u043B\u0430\u0441\u0442\u0438"
    - "\u041F\u0440\u0438\u0437\u043E\u0432\u0438\u0442\u0435 \u0441\u0442\u0440\u0430\u0436\u0430,
      \u0447\u0442\u043E\u0431\u044B \u043F\u0440\u0438\u0441\u043E\u0435\u0434\u0438\u043D\u0438\u0442\u044C\u0441\u044F
      \u043A \u0431\u0438\u0442\u0432\u0435. \u041D\u0430\u043D\u0435\u0441\u0438\u0442\u0435
      <color=#bb5c14>{0}</color> \u0443\u0440\u043E\u043D\u0430 \u043F\u0440\u0438
      \u043F\u0440\u0438\u0437\u0435\u043C\u043B\u0435\u043D\u0438\u0438 \u0432 \u0432\u044B\u0431\u0440\u0430\u043D\u043D\u043E\u0439
      \u043E\u0431\u043B\u0430\u0441\u0442\u0438. \u0423 \u043D\u0435\u0433\u043E
      <color=#bb5c14>{1}</color> \u0437\u0434\u043E\u0440\u043E\u0432\u044C\u044F
      \u0438 <color=#bb5c14>{2}</color> \u0443\u0440\u043E\u043D\u0430."
    - "\u0412\u043E\u0441\u0441\u0442\u0430\u043D\u0430\u0432\u043B\u0438\u0432\u0430\u0435\u0442<color=#bb5c14>{0}</color>
      \u043E\u0447\u043A\u0438 \u0437\u0434\u043E\u0440\u043E\u0432\u044C\u044F \u0432
      \u0441\u0435\u043A\u0443\u043D\u0434\u0443 \u0434\u043B\u044F \u0432\u0441\u0435\u0445
      \u0441\u043E\u0440\u0430\u0442\u043D\u0438\u043A\u043E\u0432 \u0432 \u043E\u0431\u043B\u0430\u0441\u0442\u0438
      \u0446\u0435\u043B\u0438. \u041F\u0440\u043E\u0434\u043E\u043B\u0436\u0438\u0442\u0435\u043B\u044C\u043D\u043E\u0441\u0442\u044C<color=#bb5c14>{1}</color>
      \u0441\u0435\u043A\u0443\u043D\u0434"
    - "\u041D\u0430 \u043F\u0443\u0442\u0438 \u0440\u0430\u0441\u0441\u0442\u0430\u0432\u043B\u0435\u043D\u044B
      \u0441\u043F\u0435\u0446\u0438\u0430\u043B\u044C\u043D\u044B\u0435 \u043C\u0438\u043D\u044B,
      \u043D\u0430\u043D\u043E\u0441\u044F\u0449\u0438\u0435 \u0443\u0440\u043E\u043D
      \u0432\u0440\u0430\u0433\u0430\u043C, \u043A\u043E\u0442\u043E\u0440\u044B\u0435
      \u043D\u0430\u0441\u0442\u0443\u043F\u0430\u044E\u0442 \u043D\u0430 \u043D\u0435\u0433\u043E
      <color=#bb5c14>{0}</color>"
    - "\u041F\u0440\u0438\u0437\u044B\u0432\u0430\u0435\u0442 \u0442\u043E\u0442\u0435\u043C
      \u0441 <color=#bb5c14>{0}</color> \u0417\u0434\u043E\u0440\u043E\u0432\u044C\u0435,
      \u043A\u043E\u0442\u043E\u0440\u044B\u0439 \u0434\u043B\u0438\u0442\u0441\u044F
      <color=#bb5c14>{1}</color> \u0441\u0435\u043A\u0443\u043D\u0434, \u0441\u043E\u0437\u0434\u0430\u0432\u0430\u044F
      \u043F\u0440\u043E\u043A\u043B\u044F\u0442\u0443\u044E \u043E\u0431\u043B\u0430\u0441\u0442\u044C,
      \u043A\u043E\u0442\u043E\u0440\u0430\u044F \u043F\u0440\u0438\u0432\u043B\u0435\u043A\u0430\u0435\u0442
      \u0432\u0441\u0435\u0445 \u0432\u0440\u0430\u0433\u043E\u0432 \u0441 \u0430\u0442\u0430\u043A\u0443\u044E\u0449\u0435\u0439
      \u0441\u043F\u043E\u0441\u043E\u0431\u043D\u043E\u0441\u0442\u044C\u044E. \u0412\u0440\u0430\u0433\u0438
      \u0432 \u043E\u0431\u043B\u0430\u0441\u0442\u0438 \u043F\u043E\u043B\u0443\u0447\u0430\u044E\u0442
      <color=#bb5c14>{2}</color> \u0443\u0440\u043E\u043D\u0430 \u0432 \u0441\u0435\u043A\u0443\u043D\u0434\u0443.\n\n*
      \u0418\u043C\u043C\u0443\u043D\u0438\u0442\u0435\u0442 \u043A \u043B\u0435\u0442\u0430\u044E\u0449\u0438\u043C
      \u0432\u0440\u0430\u0433\u0430\u043C"
    - "\u041A\u0430\u0436\u0434\u0430\u044F \u0431\u0430\u0437\u043E\u0432\u0430\u044F
      \u0430\u0442\u0430\u043A\u0430 \u0438\u043C\u0435\u0435\u0442 <color=# bb5c14>{0}%</color>
      \u0448\u0430\u043D\u0441 \u0432\u044B\u0437\u0432\u0430\u0442\u044C \u043F\u0440\u043E\u0441\u0442\u0443\u0434\u0443.\n\n*
      \u0418\u043C\u043C\u0443\u043D\u0438\u0442\u0435\u0442 \u043A \u0431\u043E\u0441\u0441\u0430\u043C
      \u0438 \u043C\u0438\u043D\u0438-\u0431\u043E\u0441\u0441\u0430\u043C"
    - "\u041E\u0441\u043D\u043E\u0432\u043D\u0430\u044F \u0430\u0442\u0430\u043A\u0430
      \u0433\u0435\u0440\u043E\u044F \u0438\u043C\u0435\u0435\u0442 <color=#bb5c14>{0}%</color>
      \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E\u0441\u0442\u0438 \u043D\u0430\u043D\u0435\u0441\u0442\u0438
      \u043E\u0436\u043E\u0433\u0438 \u0432\u0440\u0430\u0433\u0443. \u041E\u0436\u043E\u0433\u0438
      \u0434\u043B\u044F\u0442\u0441\u044F \u043D\u0435\u0441\u043A\u043E\u043B\u044C\u043A\u043E<color=#bb5c14>{1}</color>
      \u0441\u0435\u043A\u0443\u043D\u0434. \u041A\u043E\u043B\u0438\u0447\u0435\u0441\u0442\u0432\u043E
      \u043E\u0436\u043E\u0433\u043E\u0432 \u043F\u0440\u0438 \u0431\u0430\u0437\u043E\u0432\u043E\u0439
      \u0430\u0442\u0430\u043A\u0435 \u043D\u0435 \u0431\u0443\u0434\u0435\u0442
      \u0443\u0447\u0438\u0442\u044B\u0432\u0430\u0442\u044C\u0441\u044F, \u0435\u0441\u043B\u0438
      \u043E\u0436\u043E\u0433\u0438 \u0432\u0441\u0435 \u0435\u0449\u0435 \u043F\u0440\u043E\u0434\u043E\u043B\u0436\u0430\u044E\u0442\u0441\u044F"
    - "\u0410\u0442\u0430\u043A\u0438 \u0432\u0440\u0430\u0433\u043E\u0432 \u0438\u043C\u0435\u044E\u0442
      <color=#bb5c14>{0}%</color> \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E\u0441\u0442\u0438
      \u0443\u0441\u0442\u0440\u043E\u0438\u0442\u044C \u0432\u0437\u0440\u044B\u0432
      \u043F\u0440\u0438 \u043A\u0430\u0441\u0430\u043D\u0438\u0438 \u0442\u0435\u043B\u0430
      \u0433\u0435\u0440\u043E\u044F. \u0412\u0441\u0435 \u0432\u0440\u0430\u0433\u0438
      \u0432 \u0440\u0430\u0434\u0438\u0443\u0441\u0435 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u044F,
      \u0432\u043A\u043B\u044E\u0447\u0430\u044F \u043D\u0430\u043F\u0430\u0434\u0430\u044E\u0449\u0435\u0433\u043E,
      \u043F\u043E\u043B\u0443\u0447\u0430\u044E\u0442<color=#bb5c14>{1}</color>
      \u0443\u0440\u043E\u043D \u043E\u0442 \u044D\u0442\u043E\u0433\u043E \u0432\u0437\u0440\u044B\u0432\u0430"
    - "\u041A\u043E\u0433\u0434\u0430 \u043F\u043E\u044F\u0432\u043B\u044F\u0435\u0442\u0441\u044F
      \u0417\u0432\u0435\u0437\u0434\u043D\u044B\u0439 \u0421\u0442\u0440\u0430\u0436,
      \u0433\u0435\u0440\u043E\u0439 \u043F\u043E\u043B\u0443\u0447\u0430\u0435\u0442
      \u0437\u0430\u0449\u0438\u0442\u043D\u0443\u044E \u0430\u0443\u0440\u0443.
      \u0412\u043E \u0432\u0440\u0435\u043C\u044F \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u043E\u0432\u0430\u043D\u0438\u044F
      \u0417\u0432\u0435\u0437\u0434\u043D\u043E\u0433\u043E \u0421\u0442\u0440\u0430\u0436\u0430
      \u0433\u0435\u0440\u043E\u0439 \u043F\u043E\u043B\u0443\u0447\u0430\u0435\u0442
      \u0431\u043E\u043B\u044C\u0448\u0435 \u0411\u0440\u043E\u043D\u0438 \u0438
      <color=#bb5c14>{0}%</color><color=#bb5c14>{1}%</color> \u041C\u0430\u0433\u0438\u0447\u0435\u0441\u043A\u043E\u0433\u043E
      \u0421\u043E\u043F\u0440\u043E\u0442\u0438\u0432\u043B\u0435\u043D\u0438\u044F"
    - "\u0413\u0435\u0440\u043E\u0439 \u043E\u0431\u043B\u0430\u0434\u0430\u0435\u0442
      \u0441\u043F\u043E\u0441\u043E\u0431\u043D\u043E\u0441\u0442\u044C\u044E \u0432\u043E\u0441\u0441\u0442\u0430\u043D\u0430\u0432\u043B\u0438\u0432\u0430\u0442\u044C\u0441\u044F
      \u043D\u0430 <color=#bb5c14>{0}</color> \u043E\u0442 \u043C\u0430\u043A\u0441\u0438\u043C\u0430\u043B\u044C\u043D\u043E\u0433\u043E
      \u043A\u043E\u043B\u0438\u0447\u0435\u0441\u0442\u0432\u0430 \u043E\u0447\u043A\u043E\u0432
      \u0437\u0434\u043E\u0440\u043E\u0432\u044C\u044F \u0432 \u0441\u0435\u043A\u0443\u043D\u0434\u0443
      \u0434\u0430\u0436\u0435 \u0432 \u0431\u043E\u044E."
    - "\u0412\u0440\u0430\u0433\u0438, \u043A\u043E\u0442\u043E\u0440\u044B\u0435
      \u0431\u0443\u0434\u0443\u0442 \u0430\u0442\u0430\u043A\u043E\u0432\u0430\u043D\u044B
      \u043E\u0441\u043D\u043E\u0432\u043D\u043E\u0439 \u0430\u0442\u0430\u043A\u043E\u0439
      \u0433\u0435\u0440\u043E\u044F, \u0431\u0443\u0434\u0443\u0442 \u0438\u043C\u0435\u0442\u044C
      <color=#bb5c14>{0}%</color> \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E\u0441\u0442\u044C
      \u0431\u044B\u0442\u044C \u0432\u0437\u043E\u0440\u0432\u0430\u043D\u043D\u044B\u043C\u0438.
      \u0412\u0441\u0435 \u0432\u0440\u0430\u0433\u0438 \u0432 \u0433\u0440\u0430\u043D\u0438\u0446\u0430\u0445
      \u0432\u0437\u0440\u044B\u0432\u0430 \u043F\u043E\u043B\u0443\u0447\u0430\u044E\u0442<color=#bb5c14>{1}</color>
      \u0443\u0440\u043E\u043D"
    - "\u041F\u043E\u0441\u043B\u0435 \u043A\u0430\u0436\u0434\u044B\u0445 <color=#bb5c14>{0}</color>
      \u0430\u0442\u0430\u043A \u0413\u0435\u0440\u043E\u0439 \u043D\u0430\u043A\u043B\u0430\u0434\u044B\u0432\u0430\u0435\u0442
      \u043F\u0440\u043E\u043A\u043B\u044F\u0442\u0438\u0435, \u043D\u0430\u0432\u043E\u0434\u044F\u0449\u0435\u0435
      \u0443\u0436\u0430\u0441 \u043D\u0430 \u0432\u0441\u0435\u0445 \u043E\u043A\u0440\u0443\u0436\u0430\u044E\u0449\u0438\u0445
      \u0432\u0440\u0430\u0433\u043E\u0432.\n\n* \u0418\u043C\u043C\u0443\u043D\u0438\u0442\u0435\u0442
      \u043A \u043B\u0435\u0442\u0430\u044E\u0449\u0438\u043C \u0432\u0440\u0430\u0433\u0430\u043C"
    - "\u0417\u0430\u043A\u043B\u0438\u043D\u0430\u043D\u0438\u044F, \u043D\u0430\u0434\u0435\u0442\u044B\u0435
      \u043D\u0430 \u0413\u0435\u0440\u043E\u044F, \u0447\u0442\u043E\u0431\u044B
      \u0434\u043E\u0431\u0430\u0432\u0438\u0442\u044C \u0434\u043E\u043F\u043E\u043B\u043D\u0438\u0442\u0435\u043B\u044C\u043D\u044B\u0435
      \u043D\u0430\u0432\u044B\u043A\u0438 \u0432 \u0431\u043E\u044E."
    - "\u0417\u0430\u043C\u043E\u0440\u043E\u0437\u044C\u0442\u0435 \u0432\u0440\u0430\u0433\u043E\u0432
      \u0432 \u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043D\u043E\u0439
      \u043E\u0431\u043B\u0430\u0441\u0442\u0438 \u0438 \u0434\u0430\u0439\u0442\u0435
      \u0413\u0435\u0440\u043E\u044E, \u043E\u0441\u043D\u0430\u0449\u0435\u043D\u043D\u043E\u043C\u0443
      \u044D\u0442\u0438\u043C \u0417\u0430\u043A\u043B\u0438\u043D\u0430\u043D\u0438\u0435\u043C,
      \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E\u0441\u0442\u044C \u0437\u0430\u043C\u043E\u0440\u0430\u0436\u0438\u0432\u0430\u0442\u044C
      \u0432\u0440\u0430\u0433\u043E\u0432 \u043F\u0440\u0438 \u043A\u0430\u0436\u0434\u043E\u0439
      \u043E\u0441\u043D\u043E\u0432\u043D\u043E\u0439 \u0430\u0442\u0430\u043A\u0435"
    - "\u0412\u044B\u0437\u043E\u0432\u0438\u0442\u0435 \u0432\u0437\u0440\u044B\u0432
      \u0432 \u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043D\u043E\u0439
      \u043E\u0431\u043B\u0430\u0441\u0442\u0438 \u0438 \u0434\u0430\u0439\u0442\u0435
      \u0413\u0435\u0440\u043E\u044E, \u043E\u0441\u043D\u0430\u0449\u0435\u043D\u043D\u043E\u043C\u0443
      \u044D\u0442\u0438\u043C \u0437\u0430\u043A\u043B\u0438\u043D\u0430\u043D\u0438\u0435\u043C,
      \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E\u0441\u0442\u044C \u0441\u0436\u0438\u0433\u0430\u0442\u044C
      \u0432\u0440\u0430\u0433\u043E\u0432"
    - "\u0421\u043E\u0437\u0434\u0430\u0435\u0442 \u0432\u0437\u0440\u044B\u0432,
      \u043A\u043E\u0442\u043E\u0440\u044B\u0439 \u0443\u043D\u0438\u0447\u0442\u043E\u0436\u0430\u0435\u0442
      \u0432\u0440\u0430\u0433\u043E\u0432 \u0438 \u043F\u043E\u0437\u0432\u043E\u043B\u044F\u0435\u0442
      \u0413\u0435\u0440\u043E\u044E, \u043E\u0441\u043D\u0430\u0449\u0435\u043D\u043D\u043E\u043C\u0443
      \u041C\u0430\u0433\u0438\u0435\u0439, \u0432\u0437\u043E\u0440\u0432\u0430\u0442\u044C\u0441\u044F
      \u043F\u0440\u0438 \u043D\u0430\u043F\u0430\u0434\u0435\u043D\u0438\u0438"
    - "\u041F\u0440\u0438\u0437\u043E\u0432\u0438\u0442\u0435 \u0417\u0432\u0435\u0437\u0434\u043D\u044B\u0445
      \u0421\u0442\u0440\u0430\u0436\u0435\u0439 \u0438 \u0443\u0432\u0435\u043B\u0438\u0447\u044C\u0442\u0435
      \u0437\u0430\u0449\u0438\u0442\u0443 \u0413\u0435\u0440\u043E\u044F, \u043E\u0441\u043D\u0430\u0449\u0435\u043D\u043D\u043E\u0433\u043E
      \u044D\u0442\u0438\u043C \u0437\u0430\u043A\u043B\u0438\u043D\u0430\u043D\u0438\u0435\u043C"
    - "\u0418\u0441\u0446\u0435\u043B\u044F\u0435\u0442 \u0441\u043E\u0440\u0430\u0442\u043D\u0438\u043A\u043E\u0432
      \u0438 \u0434\u0430\u0435\u0442 \u0413\u0435\u0440\u043E\u044E, \u043E\u0441\u043D\u0430\u0449\u0435\u043D\u043D\u043E\u043C\u0443
      \u044D\u0442\u0438\u043C \u0437\u0430\u043A\u043B\u0438\u043D\u0430\u043D\u0438\u0435\u043C,
      \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E\u0441\u0442\u044C \u0432\u043E\u0441\u0441\u0442\u0430\u043D\u0430\u0432\u043B\u0438\u0432\u0430\u0442\u044C
      \u043E\u0447\u043A\u0438 \u0437\u0434\u043E\u0440\u043E\u0432\u044C\u044F \u0432
      \u0431\u043E\u044E"
    - "\u0420\u0430\u0441\u0441\u0442\u0430\u0432\u043B\u044F\u0439\u0442\u0435 \u043B\u043E\u0432\u0443\u0448\u043A\u0438
      \u043F\u043E \u043F\u0443\u0442\u0438 \u0438 \u0434\u0430\u0439\u0442\u0435
      \u0413\u0435\u0440\u043E\u044E \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E\u0441\u0442\u044C
      \u043D\u0430\u043D\u0435\u0441\u0442\u0438 \u0432\u0437\u0440\u044B\u0432\u043D\u0443\u044E
      \u0430\u0442\u0430\u043A\u0443"
    - "\u041F\u0440\u0438\u0437\u044B\u0432\u0430\u0435\u0442 \u0442\u043E\u0442\u0435\u043C
      \u0434\u043B\u044F \u043F\u0440\u0438\u0432\u043B\u0435\u0447\u0435\u043D\u0438\u044F
      \u0438 \u043F\u0440\u043E\u043A\u043B\u044F\u0442\u0438\u044F \u0432\u0440\u0430\u0433\u043E\u0432.
      \u0423 \u0433\u0435\u0440\u043E\u0435\u0432 \u0435\u0441\u0442\u044C \u0441\u043F\u043E\u0441\u043E\u0431\u043D\u043E\u0441\u0442\u044C
      \u0432\u044B\u0437\u044B\u0432\u0430\u0442\u044C \u0443\u0436\u0430\u0441,
      \u043A\u043E\u0433\u0434\u0430 \u043E\u043D\u0438 \u044D\u043A\u0438\u043F\u0438\u0440\u043E\u0432\u0430\u043D\u044B
      \u044D\u0442\u0438\u043C \u0437\u0430\u043A\u043B\u0438\u043D\u0430\u043D\u0438\u0435\u043C."
