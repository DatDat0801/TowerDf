    - "\u9B54\u6CD5\u5730\u96F7"
    - "\u8FDC\u53E4\u9B54\u67F1"
    - "\u5728\u9009\u5B9A\u533A\u57DF\u51BB\u7ED3\u654C\u4EBA<color=#bb5c14>{0}</color>\u79D2\u949F.
      \u51B0\u878D\u5316\u540E\uFF0C\u654C\u4EBA\u4F1A\u51B0\u51B7 <color=#bb5c14>{1}</color>
      \u79D2\n\n*\u514D\u75AB\u98DE\u884C\u654C\u4EBA"
    - "\u521B\u5EFA\u843D\u5728\u9009\u5B9A\u533A\u57DF\u7684\u96F7\u9706\u548C\u706B\u7130\uFF0C\u5E76\u9020\u6210<color=#bb5c14>{0}</color>\u4F24\u5BB3"
    - "\u5236\u9020\u7206\u70B8\uFF0C\u5BF9\u9009\u5B9A\u533A\u57DF\u5185\u7684\u654C\u4EBA\u9020\u6210<color=#bb5c14>{0}</color>\u5DE8\u5927\u4F24\u5BB3"
    - "\u53EC\u5524\u4E00\u4E2A\u5177\u6709<color=#bb5c14>{1}</color>\u8840\u548C<color=#bb5c14>{2}</color>
      \u4F24\u5BB3\u7684\u5B88\u62A4\u8005\u53C2\u4E0E\u6218\u6597\u3002\u964D\u843D\u5728\u9009\u5B9A\u533A\u57DF\u5C06\u9020\u6210<color=#bb5c14>{0}</color>\u4F24\u5BB3\u3002"
    - "\u6BCF\u79D2\u4E3A\u76EE\u6807\u533A\u57DF\u5185\u7684\u6240\u6709\u76DF\u53CB\u6062\u590D<color=#bb5c14>{0}</color>\u8840\u3002\u6301\u7EED\u65F6\u95F4<color=#bb5c14>{1}</color>\u79D2"
    - "\u5728\u8DEF\u4E0A\u653E\u7F6E\u7279\u6B8A\u5730\u96F7\uFF0C\u5BF9\u8E29\u5230\u5730\u96F7\u7684\u654C\u4EBA\u9020\u6210<color=#bb5c14>{0}</color>\u4F24\u5BB3"
    - "\u53EC\u5524\u4E00\u4E2A\u6301\u7EED<color=#bb5c14>{1}</color>\u79D2\u7684<color=#bb5c14>{0}</color>\u8840\u56FE\u817E\uFF0C\u521B\u9020\u4E00\u4E2A\u8BC5\u5492\u533A\u57DF\uFF0C\u5438\u5F15\u6240\u6709\u5177\u6709\u653B\u51FB\u80FD\u529B\u7684\u654C\u4EBA\u3002
      \u8BE5\u533A\u57DF\u5185\u7684\u654C\u4EBA\u6BCF\u79D2\u53D7\u5230 <color=#bb5c14>{2}</color>
      \u70B9\u4F24\u5BB3\u3002\n\n*\u514D\u75AB\u98DE\u884C\u654C\u4EBA"
    - "\u82F1\u96C4\u7684\u666E\u653B\u8BA9\u654C\u4EBA\u611F\u5230\u51B0\u51B7\u3002
      \u6BCF\u6B21\u653B\u51FB\u90FD\u6709 <color=#bb5c14>{0}%</color> \u51E0\u7387\u4F7F\u654C\u4EBA\u611F\u5192\u3002\n\n*\u514D\u75ABBoss\u548C\u8FF7\u4F60Boss"
    - "\u82F1\u96C4\u7684\u57FA\u672C\u653B\u51FB\u6709<color=#bb5c14>{0}%</color>\u51E0\u7387\u5BF9\u654C\u4EBA\u9020\u6210\u70E7\u4F24\u3002\u70E7\u4F24\u6301\u7EED<color=#bb5c14>{1}</color>\u79D2\u3002\u5982\u679C\u71C3\u70E7\u4ECD\u7136\u6301\u7EED\uFF0C\u5219\u6BCF\u6B21\u666E\u901A\u653B\u51FB\u7684\u71C3\u70E7\u7387\u5C06\u4E0D\u8BA1\u7B97\u5728\u5185"
    - "E\u654C\u4EBA\u7684\u653B\u51FB\u6709<color=#bb5c14>{0}%</color>\u7684\u51E0\u7387\u5728\u82F1\u96C4\u8EAB\u4E0A\u89E6\u53D1\u7206\u70B8\u3002\u5305\u62EC\u653B\u51FB\u8005\u5728\u5185\u7684\u6240\u6709\u8303\u56F4\u5185\u7684\u654C\u4EBA\u90FD\u4F1A\u53D7\u5230\u7206\u70B8\u7684<color=#bb5c14>{1}</color>\u4F24\u5BB3"
    - "\u5F53\u661F\u9645\u5B88\u62A4\u8005\u51FA\u73B0\u65F6\uFF0C\u82F1\u96C4\u4F1A\u6536\u5230\u4FDD\u62A4\u5149\u73AF\u3002\u5728\u661F\u9645\u5B88\u62A4\u8005\u5B58\u5728\u671F\u95F4\uFF0C\u82F1\u96C4\u83B7\u5F97\u66F4\u591A<color=#bb5c14>{0}%</color>\u62A4\u7532\u548C<color=#bb5c14>{1}%</color>\u9B54\u6297"
    - "\u82F1\u96C4\u5373\u4F7F\u5728\u6218\u6597\u4E2D\u4E5F\u6709\u80FD\u529B\u6BCF\u79D2\u6062\u590D\u6700\u5927\u751F\u547D\u503C<color=#bb5c14>{0}</color>"
    - "\u88AB\u82F1\u96C4\u57FA\u672C\u653B\u51FB\u6240\u653B\u51FB\u7684\u654C\u4EBA\u4F1A\u6709<color=#bb5c14>{0}%</color>\u7684\u51E0\u7387\u7206\u70B8\u3002\u7206\u70B8\u8303\u56F4\u5185\u7684\u6240\u6709\u654C\u4EBA\u90FD\u4F1A\u53D7\u5230<color=#bb5c14>{1}</color>\u4F24\u5BB3"
    - "\u5728\u6BCF\u6B21<color=#bb5c14>{0}</color> \u653B\u51FB\u540E\uFF0C\u82F1\u96C4\u90FD\u4F1A\u65BD\u653E\u4E00\u4E2A\u8BC5\u5492\uFF0C\u4F7F\u5468\u56F4\u7684\u6240\u6709\u654C\u4EBA\u90FD\u611F\u5230\u6050\u60E7\u3002\n\n*\u514D\u75AB\u98DE\u884C\u654C\u4EBA"
    - "\u82F1\u96C4\u88C5\u5907\u7684\u6CD5\u672F\uFF0C\u5728\u6218\u6597\u4E2D\u589E\u52A0\u4E3B\u52A8\u6280\u80FD"
    - "\u51BB\u7ED3\u533A\u57DF\u5185\u7684\u654C\u4EBA\uFF0C\u5E76\u4F7F\u88C5\u5907\u6B64\u6CD5\u672F\u7684\u82F1\u96C4\u6709\u673A\u4F1A\u5728\u6BCF\u6B21\u57FA\u672C\u653B\u51FB\u4E2D\u51BB\u7ED3\u654C\u4EBA"
    - "\u5728\u533A\u57DF\u5185\u5F15\u53D1\u7206\u70B8\u5E76\u4F7F\u88C5\u5907\u6B64\u6CD5\u672F\u7684\u82F1\u96C4\u80FD\u591F\u70E7\u4F24\u654C\u4EBA"
    - "\u5236\u9020\u7206\u70B8\uFF0C\u6467\u6BC1\u654C\u4EBA\uFF0C\u5E76\u8BA9\u88C5\u5907\u9B54\u6CD5\u7684\u82F1\u96C4\u5728\u53D7\u5230\u653B\u51FB\u65F6\u7206\u70B8"
    - "\u53EC\u5524\u661F\u9645\u5B88\u62A4\u8005\uFF0C\u63D0\u5347\u88C5\u5907\u6B64\u6CD5\u672F\u7684\u82F1\u96C4\u7684\u9632\u5FA1\u529B"
    - "\u6CBB\u7597\u76DF\u53CB\u5E76\u8D4B\u4E88\u88C5\u5907\u6B64\u6CD5\u672F\u7684\u82F1\u96C4\u5728\u6218\u6597\u4E2D\u6062\u590D\u751F\u547D\u503C\u7684\u80FD\u529B"
    - "\u6CBF\u9014\u8BBE\u7F6E\u9677\u9631\uFF0C\u4F7F\u82F1\u96C4\u6709\u80FD\u529B\u9020\u6210\u7206\u70B8\u6027\u653B\u51FB"
    - "\u53EC\u5524\u4E00\u4E2A\u56FE\u817E\u6765\u5438\u5F15\u548C\u8BC5\u5492\u654C\u4EBA\u3002
      \u82F1\u96C4\u5728\u88C5\u5907\u6B64\u6CD5\u672F\u65F6\u6709\u80FD\u529B\u9020\u6210\u6050\u60E7\u3002"
